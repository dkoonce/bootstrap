#Install kpt
curl -Lo kpt https://github.com/GoogleContainerTools/kpt/releases/download/v1.0.0-beta.13/kpt_linux_amd64
sudo install -o root -m 0755 kpt /usr/local/bin/
#echo "complete -C /usr/local/bin/kpt kpt" | sudo tee /usr/local/share/bash-completion/completions/kpt
echo "source <(kpt completion bash)" | sudo tee /usr/local/share/bash-completion/completions/kpt
